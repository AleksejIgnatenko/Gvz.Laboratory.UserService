    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>b4327e40-cfa1-41fc-8b30-7bd3182f2cf5</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
